<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kosamattom.cusmateogl"
    xmlns:tools="http://schemas.android.com/tools"
    >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name="com.kosamattom.cusmateogl.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon"
        android:usesCleartextTraffic="true">
        <activity android:name="com.kosamattom.cusmateogl.payment.axis_payment.AxisPaymentActivity"></activity>
        <activity android:name="com.kosamattom.cusmateogl.payout.PayoutActivity" />
        <activity android:name="com.kosamattom.cusmateogl.account_list.dialog_inventory_details.InventoryDetailsActivity" />
        <activity
            android:name="com.kosamattom.cusmateogl.signup.SignupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.reset_password.otp.OtpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.reset_password.reset_password.ResetPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.reset_password.forgot_password.ForgotPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.payment.paytm.PayUActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.contact.ContactActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.renew_loan.RenewLoanActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.final_confirmation.ConfirmationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.calculator.CalculatorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.finialize_amount.FinalizeAmountActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.gold_loan.select_scheme.SelectSchemeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.account_details.closed_loans.ClosedLoansActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.gold_loan.GoldLoanActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.payment.paytm.PaymentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.select_payment.SelectPaymentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.account_details.AccountDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.account_list.AccountListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.home.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.kosamattom.cusmateogl.login.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kosamattom.cusmateogl.MainActivity"
            android:screenOrientation="portrait" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>